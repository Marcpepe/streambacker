{
  "name": "streambacker",
  "private": true,
  "version": "0.0.13",
  "description": "Streambacker app",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git@github.com:Marcpepe/streambacker.git"
  },
  "scripts": {
    "build": "./node_modules/.bin/gulp build",
    "watch": "./node_modules/.bin/gulp watch",
    "test": "npm run test:server",
		"test:server": "NODE_ENV=test mocha test 'test/server/**/*.coffee'",
    "test:server:unit": "NODE_ENV=test mocha test 'test/server/unit/**/*.coffee'",
    "test:server:func": "NODE_ENV=test mocha test 'test/server/functional/**/*.coffee'",
    "test:server:coverage": "export testing=1;mocha test 'test/app/**/*.coffee' --reporter html-cov > test/coverage-back/coverage.html",
    "test:client:unit": "./node_modules/karma/bin/karma start test/client/karma-unit.coffee --single-run",
    "test:client:integration": "./node_modules/.bin/protractor test/client/protractor.conf.coffee"
  },
  "dependencies": {
    "bower": "*",
    "coffee-script": "*",
    "compression": "*",
    "cors": "*",
    "ejs": "*",
    "errorhandler": "*",
    "http-proxy": "*",
    "include-all": "*",
    "rc": "*",
    "run-sequence": "*",
    "sails": "*",
    "sails-disk": "*",
    "serve-static": "*",
    "ws": "^0.8.0"
  },
  "devDependencies": {
    "chai": "*",
    "cucumber": "*",
    "del": "*",
    "gulp": "*",
    "gulp-angular-templatecache": "*",
    "gulp-autoprefixer": "*",
    "gulp-coffee": "*",
    "gulp-coffeelint": "*",
    "gulp-concat": "*",
    "gulp-filter": "*",
    "gulp-jade": "*",
    "gulp-less": "3.0.3",
    "gulp-minify-css": "*",
    "gulp-ng-annotate": "*",
    "gulp-nodemon": "*",
    "gulp-plumber": "*",
    "gulp-rename": "*",
    "gulp-sourcemaps": "*",
    "gulp-uglify": "*",
    "gulp-util": "*",
    "karma": "^0.13.14",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coffee-preprocessor": "^0.3.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.0",
    "karma-ng-jade2js-preprocessor": "^0.2.1",
    "main-bower-files": "*",
    "mocha": "^2.3.3",
    "protractor": "*",
    "require-dir": "*"
  },
  "main": "app.js",
  "license": ""
}
