{
  "name": "streambacker",
  "private": true,
  "version": "0.0.1",
  "description": "Streambacker app",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git@github.com:Marcpepe/streambacker.git"
  },
  "scripts": {
    "postinstall": "./node_modules/.bin/bower install && ./node_modules/.bin/gulp build",
    "build": "./node_modules/.bin/gulp build",
    "start": "./node_modules/.bin/gulp watch",
    "start:all": "npm run start:server & ./node_modules/.bin/gulp watch",
    "start:server": "./node_modules/.bin/coffee server.coffee & ./node_modules/.bin/coffee server/notsoshitty-server.coffee",
    "test": "karma start test/karma-unit.coffee --single-run",
    "test:unit": "karma start test/karma-unit.coffee",
    "test:functional": "node_modules/.bin/protractor --require coffee-script/register test/protractor.conf.coffee",
    "docs": "gulp docs"
  },
  "dependencies": {
    "ejs": "~0.8.4",
    "include-all": "~0.1.3",
    "rc": "~0.5.0",
    "sails": "~0.11.0",
    "sails-disk": "~0.10.0",
    "bower": "*",
    "coffee-script": "*",
    "compression": "~1.5.1",
    "cors": "^2.7.1",
    "errorhandler": "~1.4.1",
    "http-proxy": "^1.7.1",
    "run-sequence": "*",
    "serve-static": "^1.7.1"
  },
  "devDependencies": {
    "chai": "^3.0.0",
    "cucumber": "^0.5.0",
    "del": "*",
    "gulp": "*",
    "gulp-angular-templatecache": "*",
    "gulp-autoprefixer": "*",
    "gulp-coffee": "*",
    "gulp-coffeelint": "*",
    "gulp-concat": "*",
    "gulp-filter": "*",
    "gulp-jade": "*",
    "gulp-less": "*",
    "gulp-minify-css": "*",
    "gulp-ng-annotate": "*",
    "gulp-nodemon": "*",
    "gulp-plumber": "*",
    "gulp-rename": "*",
    "gulp-sourcemaps": "*",
    "gulp-uglify": "~1.2.0",
    "gulp-util": "*",
    "main-bower-files": "^2.1.0",
    "protractor": "^2.1.0",
    "require-dir": "*",
    "vinyl-paths": "*"
  },
  "main": "app.js",
  "license": ""
}
