{
  "name": "streambacker",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git@github.com:Marcpepe/streambacker.git"
  },
  "scripts": {
    "postinstall": "bower install && gulp build",
    "start": "gulp watch",
    "start:all": "npm run start:server & gulp watch",
    "start:server": "node_modules/.bin/coffee server.coffee & node_modules/.bin/coffee server/notsoshitty-server.coffee",
    "test": "karma start test/karma-unit.coffee --single-run",
    "test:unit": "karma start test/karma-unit.coffee",
    "test:functional": "node_modules/.bin/protractor --require coffee-script/register test/protractor.conf.coffee",
    "docs": "gulp docs"
  },
  "dependencies": {
    "bower": "*",
    "coffee-script": "*",
    "compression": "~1.5.1",
    "cors": "^2.7.1",
    "errorhandler": "~1.4.1",
    "express": "^4.10.4",
    "gulp": "*",
    "gulp-angular-templatecache": "*",
    "gulp-coffee": "*",
    "gulp-concat": "*",
    "gulp-filter": "*",
    "gulp-jade": "*",
    "gulp-less": "*",
    "gulp-plug": "*",
    "gulp-plumber": "*",
    "gulp-rename": "*",
    "gulp-replace": "*",
    "gulp-rimraf": "*",
    "gulp-sourcemaps": "*",
    "gulp-webserver": "*",
    "http-proxy": "^1.7.1",
    "lodash": "^3.9.3",
    "loopback": "^2.18.0",
    "loopback-boot": "^2.8.0",
    "loopback-explorer": "^1.8.0",
    "loopback-connector-mongodb": "^1.9.2",
    "loopback-datasource-juggler": "^2.29.2",
    "serve-static": "^1.7.1"
  },
  "devDependencies": {
    "chai": "^3.0.0",
    "cucumber": "^0.5.0",
    "lodash": "^3.9.3",
    "main-bower-files": "^2.1.0",
    "protractor": "^2.1.0"
  }
}
