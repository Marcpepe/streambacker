{
  "name": "streambacker",
  "private": true,
  "version": "0.0.1",
  "description": "Streambacker app",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git@github.com:Marcpepe/streambacker.git"
  },
  "scripts": {
    "postinstall": "./node_modules/bower/bin/bower install && ./node_modules/.bin/gulp build",
    "build": "./node_modules/.bin/gulp build",
    "watch": "./node_modules/.bin/gulp watch",
    "start:all": "npm run start:server & npm run watch",
    "start:server": "./node_modules/.bin/coffee server.coffee & ./node_modules/.bin/coffee server/notsoshitty-server.coffee",
    "test": "karma start test/client/unit/karma-unit.coffee --single-run",
    "test:unit": "karma start test/karma-unit.coffee",
    "test:functional": "node_modules/.bin/protractor --require coffee-script/register test/protractor.conf.coffee",
    "docs": "gulp docs"
  },
  "dependencies": {
    "bower": "*",
    "coffee-script": "*",
    "compression": "*",
    "cors": "*",
    "ejs": "*",
    "errorhandler": "*",
    "http-proxy": "*",
    "include-all": "*",
    "rc": "*",
    "run-sequence": "*",
    "sails": "*",
    "sails-disk": "*",
    "serve-static": "*",
    "ws": "^0.8.0"
  },
  "devDependencies": {
    "chai": "*",
    "cucumber": "*",
    "del": "*",
    "gulp": "*",
    "gulp-angular-templatecache": "*",
    "gulp-autoprefixer": "*",
    "gulp-coffee": "*",
    "gulp-coffeelint": "*",
    "gulp-concat": "*",
    "gulp-filter": "*",
    "gulp-jade": "*",
    "gulp-less": "*",
    "gulp-minify-css": "*",
    "gulp-ng-annotate": "*",
    "gulp-nodemon": "*",
    "gulp-plumber": "*",
    "gulp-rename": "*",
    "gulp-sourcemaps": "*",
    "gulp-uglify": "*",
    "gulp-util": "*",
    "karma": "^0.13.14",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coffee-preprocessor": "^0.3.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.0",
    "karma-ng-jade2js-preprocessor": "^0.2.1",
    "main-bower-files": "*",
    "mocha": "^2.3.3",
    "protractor": "*",
    "require-dir": "*"
  },
  "main": "app.js",
  "license": ""
}
