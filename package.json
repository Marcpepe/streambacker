    "gulp-angular-templatecache": "*",
    "gulp-coffee": "*",
    "gulp-concat": "*",
    "gulp-filter": "*",
    "gulp-jade": "*",
    "gulp-less": "*",
    "gulp-plug": "*",
    "gulp-plumber": "*",
    "gulp-rename": "*",
    "gulp-replace": "*",
    "gulp-rimraf": "*",
    "gulp-sourcemaps": "*",
    "gulp-webserver": "*",
    "http-proxy": "^1.7.1",
    "serve-static": "^1.7.1"
  },
  "devDependencies": {
    "chai": "^3.0.0",
